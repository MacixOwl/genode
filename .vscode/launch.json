    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug x86 - monkey_lab",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/x86_64/app/monkey_lab/monkey_lab",
            "miDebuggerServerAddress": "localhost:1234",
            "miDebuggerPath": "/usr/local/genode/tool/23.05/bin/genode-x86-gdb",
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "stopAtEntry": false,
            "MIMode": "gdb",
            "logging": {
                "engineLogging": true // Set to true to enable GDB engine logging
            },
            "preLaunchTask": "debug_monkey_lab"
        }
    ]

}
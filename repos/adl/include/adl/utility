#pragma once

namespace adl {


template<class U, class V>
struct pair {
    U first;
    V second;
    pair(const U& first = U(), const V& second = V()) : first(first), second(second) {}
};


template<class U, class V>
inline pair<U, V> make_pair(const U& first, const V& second) {
    return pair {first, second};
}


}  // namespace adl
